.eps.gif:
	@-test -d $(buildpicdir) || mkdir $(buildpicdir)
	$(CONVERT) -geometry $(IMAGE_WIDTH) $< $@

.eps.jpg:
	@-test -d $(buildpicdir) || mkdir $(buildpicdir)
	$(CONVERT) -geometry $(IMAGE_WIDTH) -quality 100 $< $@

